{
  "name": "hax-body-w-deep",
  "version": "1.0.0",
  "description": "HAX body with DEEP-UI",
  "keywords": [
    "aurelia"
  ],
  "homepage": "https://github.voya.net/Voya/financial-wellness-ui",
  "bugs": {
    "url": "https://github.voya.net/Voya/financial-wellness-ui/issues"
  },
  "license": "Voya",
  "author": "Voya",
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.voya.net/Voya/financial-wellness-ui"
  },
  "scripts": {
    "build": "npm install && npm install selenium-standalone@6.11.0 && jspm install && gulp ci:selenium:install && gulp build:ci",
    "test": "gulp test",
    "e2e": "gulp serve e2e",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js": [
      "./node_modules/.bin/eslint",
      "./node_modules/.bin/prettier --single-quote --write",
      "git add"
    ]
  },
  "devDependencies": {
    "jspm": "0.17.0-beta.45",
    "systemjs": "0.20.19",
    "voya-builder": "git+ssh://git@github.voya.net:Voya/deep-ui-voya-builder.git#v1.9.0-beta2",
    "voya-stub-server": "git+ssh://git@github.voya.net:Voya/voya-stub-server.git#v2.0.2"
  },
  "jspm": {
    "main": "main.js",
    "directories": {
      "lib": "src"
    },
    "dependencies": {
      "babel-plugin-transform-decorators-legacy": "npm:babel-plugin-transform-decorators-legacy@^1.3.4",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.25"
    },
    "devDependencies": {},
    "peerDependencies": {
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "branding": "voya-github:Voya/deep-ui-branding@1.9.1",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "debounce": "npm:debounce@^1.0.0",
      "dlv": "npm:dlv@^1.1.0",
      "dom-delegate": "npm:dom-delegate@^2.0.3",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0",
      "voya-base-styles": "voya-github:Voya/deep-ui-voya-base-styles@2.7.2",
      "voya-button": "voya-github:Voya/deep-ui-voya-button@5.4.3",
      "voya-component-utils": "voya-github:Voya/deep-ui-voya-component-utils@3.5.4",
      "voya-content": "voya-github:i702118/deep-ui-voya-content@hax",
      "voya-expand-collapse": "voya-github:Voya/deep-ui-voya-expand-collapse@2.4.0",
      "voya-notification": "voya-github:Voya/deep-ui-voya-notification@^3.0.0",
      "whatwg-fetch": "npm:whatwg-fetch@^1.0.0"
    },
    "overrides": {
      "npm:debug@2.6.9": {
        "main": "src/browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./src/browser.js": {
            "node": "./src/node.js"
          },
          "./node.js": {
            "browser": "./src/browser.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:font-awesome@4.6.3": {
        "main": "css/font-awesome.css!",
        "jspmNodeConversion": false,
        "registry": "jspm",
        "dependencies": {
          "css": "0.1"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:lodash@4.17.10": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:pbkdf2@3.0.16": {
        "main": "browser.js"
      },
      "npm:safe-buffer@5.1.2": {
        "browser": "index.js"
      }
    }
  },
  "dependencies": {
    "aurelia-cookie": "^1.0.8",
    "gulp-sass": "2.2.0"
  }
}
